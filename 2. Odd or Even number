#include <stdio.h>

int main() {
    int n, i, num;

    // Get the number of elements
    printf("Enter the number of elements: ");
    scanf("%d", &n);

    // Iterate through the set of numbers
    printf("Enter %d numbers:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &num);

        // Check if the number is odd or even
        if (num % 2 == 0)
            printf("%d is Even\n", num);
        else
            printf("%d is Odd\n", num);
    }

    return 0;
}
